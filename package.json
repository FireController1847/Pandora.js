{
  "name": "pandora.js",
  "version": "0.1.6",
  "description": "A Node.js Port of the Unofficial Pandora Radio API",
  "main": "./src/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "snekfetch": "^4.0.4"
  },
  "devDependencies": {
    "documentation": "^8.1.2",
    "eslint": "^5.9.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "test": "run-s lint && istanbul cover node_modules/mocha/bin/_mocha -- -t 16000 test/test.js",
    "docs": "documentation build src/** -f html -o out",
    "docs:json": "documentation build src/** -f json -o out/docs.json",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireController1847/node-pandora-api.git"
  },
  "keywords": [
    "pandora",
    "pandora radio",
    "pandora api",
    "pandora music",
    "music",
    "radio"
  ],
  "author": "FireController1847",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/FireController1847/node-pandora-api/issues"
  },
  "homepage": "https://github.com/FireController1847/node-pandora-api#readme"
}
